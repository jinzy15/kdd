    "import pandas as pd\n",
    "now = pd.read_csv('submission/5.10/bj.csv')\n",
    "data = pd.DataFrame()\n",
    "for i,j in now.groupby(now['station_id']):\n",
    "    j = j.sort_values(by=['time'])\n",
    "    data = data.append(j)"
   ]
